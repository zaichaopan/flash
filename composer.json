{
    "name": "zaichaopan/flash",
    "description": "A simple package to make flash message more flexible",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zaichaopan",
            "email": "zaichaopan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zaichaopan\\Flash\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5.0",
        "phpunit/phpunit": "^7.2"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/FlashTest.php"
        ]
    },
    "minimum-stability": "stable"
}
